{
  "name": "@tsgonest/types",
  "version": "0.0.1",
  "description": "Zero-runtime branded types for tsgonest validation constraints â€” type-safe alternative to JSDoc tags",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./tags": {
      "import": {
        "types": "./dist/tags.d.mts",
        "default": "./dist/tags.mjs"
      },
      "require": {
        "types": "./dist/tags.d.cts",
        "default": "./dist/tags.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "test": "tsc --noEmit"
  },
  "keywords": [
    "typescript",
    "validation",
    "branded-types",
    "phantom-types",
    "tsgonest",
    "type-safe"
  ],
  "sideEffects": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tsgonest/tsgonest"
  },
  "devDependencies": {
    "tsdown": "^0.20.3",
    "typescript": "^5.7.0"
  }
}

  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "tsc --noEmit"
  },
  "keywords": [
    "typescript",
    "validation",
    "branded-types",
    "phantom-types",
    "tsgonest",
    "type-safe"
  ],
  "sideEffects": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tsgonest/tsgonest"
  },
  "devDependencies": {
    "typescript": "^5.7.0"
  }
}
